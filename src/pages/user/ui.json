			"type": "Control",
			"scope": "#/properties/email"
		},
		{
			"type": "Control",
			"scope": "#/properties/password",
			"options": {
				"format": "password"
			}
		},
		{
			"type": "Control",
			"scope": "#/properties/name"
		},
		{
			"type": "Control",
